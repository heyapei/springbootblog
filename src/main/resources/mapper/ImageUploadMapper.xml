<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hyp.mapper.ImageUploadMapper" >
  <resultMap id="BaseResultMap" type="com.hyp.pojo.ImageUpload" >
    <result column="imgPath" property="imgpath" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.hyp.pojo.ImageUpload" useGeneratedKeys="true" keyProperty="imgPath">
    insert into imgUploadInfo (imgPath)
    values (#{imgpath,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.hyp.pojo.ImageUpload" useGeneratedKeys="true" keyProperty="imgPath">
    insert into imgUploadInfo
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="imgpath != null" >
        imgPath,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="imgpath != null" >
        #{imgpath,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>

  <insert id="insertImgPath" parameterType="string">
    insert into imgUploadInfo (imgPath)
    values (#{imgPath})
  </insert>
</mapper>